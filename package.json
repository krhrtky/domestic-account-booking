{
  "name": "domestic-account-booking",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:e2e": "node --env-file=.env.local node_modules/.bin/playwright test",
    "test:e2e:ui": "node --env-file=.env.local node_modules/.bin/playwright test --ui",
    "test:e2e:debug": "node --env-file=.env.local node_modules/.bin/playwright test --debug",
    "test:e2e:demo": "node --env-file=.env.local node_modules/.bin/playwright test e2e/demo",
    "test:e2e:demo:ui": "node --env-file=.env.local node_modules/.bin/playwright test e2e/demo --ui",
    "test:e2e:a11y": "node --env-file=.env.local node_modules/.bin/playwright test e2e/accessibility",
    "test:e2e:a11y:ui": "node --env-file=.env.local node_modules/.bin/playwright test e2e/accessibility --ui",
    "test:e2e:security": "node --env-file=.env.local node_modules/.bin/playwright test e2e/security",
    "test:e2e:security:ui": "node --env-file=.env.local node_modules/.bin/playwright test e2e/security --ui",
    "test:e2e:firefox": "node --env-file=.env.local node_modules/.bin/playwright test --project=firefox",
    "test:e2e:webkit": "node --env-file=.env.local node_modules/.bin/playwright test --project=webkit",
    "test:e2e:all": "node --env-file=.env.local node_modules/.bin/playwright test",
    "type-check": "tsc --noEmit",
    "lighthouse": "bash scripts/run-lighthouse.sh",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "check:arch": "depcruise src app --config .dependency-cruiser.js --output-type err-long",
    "check:arch:graph": "depcruise src app --config .dependency-cruiser.js --output-type dot | dot -T svg > dependency-graph.svg",
    "check:arch:html": "depcruise src app --config .dependency-cruiser.js --output-type err-html > dependency-report.html",
    "test:vrt": "node --env-file=.env.local.e2e node_modules/.bin/playwright test e2e/vrt --project=chromium-unauth",
    "test:vrt:update": "node --env-file=.env.local.e2e node_modules/.bin/playwright test e2e/vrt --project=chromium-unauth --update-snapshots",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:storybook:vrt": "STORYBOOK_VRT=true test-storybook",
    "test:storybook:vrt:update": "STORYBOOK_VRT=true test-storybook --updateSnapshot"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.8",
    "papaparse": "^5.4.1",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/eslintrc": "^3.3.3",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/nextjs-vite": "^10.1.10",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/papaparse": "^5.3.15",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "dependency-cruiser": "^17.3.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "eslint-plugin-storybook": "^10.1.10",
    "http-server": "^14.1.1",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "^9.0.2",
    "postcss": "^8.4.49",
    "storybook": "^10.1.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^7.3.0",
    "vitest": "^2.1.8",
    "wait-on": "^8.0.1"
  }
}
